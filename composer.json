{
    "name": "root/currency-rates",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": { "CurrencyRates\\": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": {"features\\CurrencyRates\\": "features/bootstrap"}
    },
    "require": {
        "php": ">=7.1.9",
        "symfony/symfony": "2.8.*",
        "twig/twig": "1.34.4",
        "doctrine/orm": "~2.5",
        "doctrine/dbal": "~2.5",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/annotations": "1.2.7",
        "twig/extensions": "~1.0",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "nixilla/php-jwt": "^0.1.1",
        "guzzlehttp/guzzle": "^6.2",
        "friendsofsymfony/rest-bundle": "^2.0",
        "doctrine/migrations": "~1.1.0",
        "doctrine/doctrine-migrations-bundle": "~1.1.0",
        "lexik/jwt-authentication-bundle": "^1.6",
        "stof/doctrine-extensions-bundle": "^1.2",
        "eightpoints/guzzle-bundle": "^5.3",
        "hwi/oauth-bundle": "^0.5.3",
        "nelmio/cors-bundle": "^1.5",
        "fadion/fixerio": "^1.2"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7",
        "behat/behat": "^3.1",
        "behat/symfony2-extension": "^2.1",
        "fesor/json_matcher": "^0.2.3",
        "phpunit/phpunit": "^6.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    },
    "minimum-stability": "alpha",
    "prefer-stable" : true
}
